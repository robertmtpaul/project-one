<%= form_for @startup, html: {multipart: true} do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <br>

  <%= f.label :description %><br>
  <%= f.text_field :description %>
  <br>

  <%= f.label :valuation %><br>
  <%= f.text_field :valuation %>
  <br>

  <%= f.label :funding %><br>
  <%= f.text_field :funding %>
  <br>

  <%= f.label :status %><br>
  <%= f.text_field :status %>
  <br>

  <%= f.label :employees %><br>
  <%= f.text_field :employees %>
  <br>
  
  <%= f.label :image, 'Image URL' %><br>
  <%= f.cl_image_upload :image %>


  <%= f.submit %>

<% end %>