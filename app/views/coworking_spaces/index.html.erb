<h2> Best coworking spaces for digital nomads </h2>

<div class="card-columns">
   <% @coworking_spaces.each do |c| %>
     <div class="card">
       <div class="card-block">
        <%= link_to coworking_space_path(c.id) do %>
            <%= cl_image_tag(c.image, class: 'w-100 p-3') %>
            <p class="card-title"><b><%= c.name %></b></p>
            <p class="card-text"><%= c.description %></p>
        <% end %>
       </div>
     </div>
   <% end %>
</div>


<div class="container">
    <% @coworking_spaces.each do |c| %>
        <div class="card">
            <%= link_to coworking_space_path(c.id) do %>
                <%= image_tag(c.image, class: 'icons') %>
                <strong><%= c.name %></strong>
                <ul class='icons'>
                <% startups.each do |s| %>
                    <li> 
                    <%= link_to startup_path(s.id) do %>
                        <%= image_tag(s.image, class: 'icons') %>
                    </li>
                    <% end%>
                <% end %>
                </ul>
            <% end %> 
        </div>
    <% end %> 
    </div> 
