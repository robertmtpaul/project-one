<%= form_for @coworking_space do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <br>

  <%= f.label :description %><br>
  <%= f.text_field :description %>
  <br>

  <%= f.label :reviews %><br>
  <%= f.text_field :reviews %>
  <br>

  <%= f.label :companies %><br>
  <%= f.text_field :companies %>
  <br>
  
  <%= f.label :image, 'Image URL' %><br>
  <%= f.cl_image_upload :image %>

  <%= f.submit %>

<% end %>