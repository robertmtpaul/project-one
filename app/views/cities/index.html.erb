
<div class="container">
    <% @cities.each do |city, startups| %>
        <div class="card">
            <%= link_to city_path(city.id) do %>
                <%= image_tag(city.image1, class: 'icons') %>
                <strong><%= city.name %></strong>
                <ul class="icons">
                <% startups.each do |s| %>
                    <li> 
                    <%= link_to startup_path(s.id) do %>
                    <%= image_tag(s.image, class: 'icons') %>
                    </li>
                    <% end%>
                <% end %>
                </ul>
            <% end %> 
        </div>
    </div> 
    <% end %> 


